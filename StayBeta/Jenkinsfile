node {
stage ('Git Checkout'){
git "https://github.com/Kiranmaiadams/CBE-Automation"
}
stage ('Build Project'){
bat "mvn -f StayBeta/pom.xml clean"
}
 stage ('Run Test'){
bat "mvn -f StayBeta/pom.xml test"
}

stage ('Publish Report'){
publishHTML([allowMissing: false, alwaysLinkToLastBuild: true, keepAll: true, reportDir: 'StayBeta/Reports/', reportFiles: 'STA-Automation.html', reportName: '1SourceReleaseReport', reportTitles: ''])
}
}