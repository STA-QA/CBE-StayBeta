<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Amendments & Cancellations - Other Data Sources" verbose="2" parallel="false">
	<listeners>
		<listener class-name="Base.staNGlistener" />
	</listeners>
	<test name="Regression">
		<classes>
			<class name="cucumberOptions.Amend_AddPassenger" />
			<class name="cucumberOptions.Amend_ChangeStartDate" />
			<class name="Ament_RemovePassengerAfterBooking" />
			<class name="cucumberOptions.Cancel_InsideCancPeriodRunner" />
			<class name="cucumberOptions.Cancel_OutsideCancPeriodRunner" />
		</classes>
	</test>
</suite>